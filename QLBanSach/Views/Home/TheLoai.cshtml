
@model IEnumerable<QLBanSach.Models.THELOAI>
<div class="col-lg-3">
    <h1 class="my-4">Thể loại</h1>
    <div class="list-group">
        @foreach (var s in Model)
        {
            <a href="@Url.Action("SPTheoTL", "Home", new { id = s.matheloai })" class="list-group-item">
                @s.tentheloai
            </a>
        }
    </div>
</div>







